{
  "workbench.colorTheme": "Dracula",
  "yaml.schemas": {
    "file:///home/anderson/.vscode/extensions/atlassian.atlascode-3.0.3/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "workbench.iconTheme": "material-icon-theme",
